geom_violin(width=1, trim=TRUE, fill="#87CEFA") +
scale_y_log10() +
geom_boxplot(width=0.7,alpha=0.7) + ggtitle("Relative Breaking Changes") + xlab("Code Samples") + ylab("Percent of BC (log scale)") +
annotate("text", x = 1, y = 4, label = "2", size = 6) +
annotate("text", x = 2, y = 44, label = "12", size = 6) +
theme(plot.title=element_text(size=20,face="bold") ,axis.title=element_text(size=18),axis.text=element_text(size=18))
p1
